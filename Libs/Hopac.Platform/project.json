{
  "version": "0.3.13",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "define": [ "TRACE" ],
    "xmlDoc": true,
    "compile": {
      "includeFiles": [
        "../Hopac.Platform.Net/AssemblyInfo.fs",
        "../Hopac.Platform.Net/Init.fs"
      ]
    },
    "additionalArguments": [
      "--warnon:1182"
    ]
  },
  "dependencies": {
    "FSharp.Core": "4.0.1.7-alpha",
    "Hopac.Core": { "target": "project" }
  },
  "frameworks": {
    "net46": {},
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
      },
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    },
    "dotnet-mergenupkg": {
      "target": "package",
      "version": "1.0.*"
    }
  }
}
